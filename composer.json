    "keywords": ["cURL", "HTTP", "Multi-thread", "parallel", "spider", "bot", "laravel"],
    "homepage": "http://docs.jyggen.com/curl",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas.stendahl@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": { "jyggen\\Curl": "lib/" }
